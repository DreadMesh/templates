  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/Viren070/AIOStreams/main/packages/frontend/public/assets/logo.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1751169689</DateInstalled>
  <DonateText/>
  <DonateLink/>
