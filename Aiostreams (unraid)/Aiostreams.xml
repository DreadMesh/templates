<?xml version="1.0"?>
<Container version="2">
  <Name>AIOStreams</Name>
  <Repository>viren070/aiostreams</Repository>
  <Registry>https://github.com/Viren070/AIOStreams</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://discord.gg/aiostreams</Support>
  <Project>https://github.com/Viren070/AIOStreams</Project>
  <Overview>AIOStreams consolidates multiple Stremio addons and debrid services into a single, easily configurable addon. It allows highly customisable filtering, sorting, and formatting of results and supports proxying all your streams through MediaFlow Proxy or StremThru for improved compatibility and IP restriction bypassing. &#xD;
&#xD;
https://discord.gg/aiostreams</Overview>
  <Category>MediaApp:Video</Category>
  <WebUI>http://[IP]:[PORT:3000]</WebUI>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/Viren070/AIOStreams/main/packages/frontend/public/assets/logo.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1751173239</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="Container Port 1" Target="3000" Default="3000" Mode="tcp" Description="" Type="Port" Display="always" Required="false" Mask="false">3000</Config>
  <Config Name="Appdata" Target="/app/data" Default="" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/aiostreams/</Config>
  <Config Name="DATABASE_URI" Target="DATABASE_URI" Default="" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">sqlite:///app/data/db.sqlite</Config>
  <Config Name="SECRET_KEY" Target="SECRET_KEY" Default="" Mode="" Description="(REQUIRED) Open the console and use the command to obtain a key:&#13;&#10;openssl rand -hex 32" Type="Variable" Display="always" Required="false" Mask="false"/>
  <Config Name="ADDON_PASSWORD" Target="ADDON_PASSWORD" Default="" Mode="" Description="Leave empty if you don't want a password protected addon" Type="Variable" Display="always" Required="false" Mask="false"/>
  <Config Name="LOG_LEVEL" Target="LOG_LEVEL" Default="http|debug" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">http</Config>
  <Config Name="LOG_TIMEZONE" Target="LOG_TIMEZONE" Default="asia/riyadh|America/New_York|Europe/London" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">asia/riyadh</Config>
  <TailscaleStateDir/>
</Container>
