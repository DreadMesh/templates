  <Config Name="Container Port 1" Target="3000" Default="3000" Mode="tcp" Description="" Type="Port" Display="always" Required="false" Mask="false">3000</Config>
  <Config Name="Container Variable 1" Target="NODE_VERSION" Default="22.16.0" Mode="{3}" Description="" Type="Variable" Display="always" Required="false" Mask="false">22.16.0</Config>
  <Config Name="Container Variable 2" Target="YARN_VERSION" Default="1.22.22" Mode="{3}" Description="" Type="Variable" Display="always" Required="false" Mask="false">1.22.22</Config>
